// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Fragrances {
  id BigInt @id
  item String
  description String?
  category String?
  createdDate DateTime?
  updatedDate DateTime?
  imageURL String?
}